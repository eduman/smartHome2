{
  "id": "demo-dgw",
  "description": "Example Gateway",
  "publicAddr": "fqdn-of-the-host",
  "staticDir": "./static",
  "catalog": [
    {
      "discover": false,
      "endpoint": "http://remotehost:8080/rc"
    }
  ],
  "http": {
    "bindAddr": "0.0.0.0",
    "bindPort": 8080
  },
  "protocols": {
    "REST": {
      "location": "/rest"
    },
    "MQTT": {
      "url": "tcp://192.168.1.254:1883",
      "prefix": "/smarthome"
    }
  }
}